{
  "src/lib/components/Tvlwc.react.js": {
    "description": "Tradingview Lightweight Chart object",
    "displayName": "Tvlwc",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID of this component"
      },
      "data": {
        "type": {
          "name": "arrayOf",
          "value": {
            "name": "shape",
            "value": {
              "seriesData": {
                "name": "arrayOf",
                "value": {
                  "name": "object"
                },
                "required": false
              },
              "seriesType": {
                "name": "enum",
                "value": [
                  {
                    "value": "'Candlestick'",
                    "computed": false
                  },
                  {
                    "value": "'Area'",
                    "computed": false
                  }
                ],
                "required": false
              },
              "seriesOptions": {
                "name": "object",
                "required": false
              }
            }
          }
        },
        "required": false,
        "description": "The data for the series"
      },
      "width": {
        "type": {
          "name": "union",
          "value": [
            {
              "name": "string"
            },
            {
              "name": "number"
            }
          ]
        },
        "required": false,
        "description": "Sets width of the parent div of the chart",
        "defaultValue": {
          "value": "600",
          "computed": false
        }
      },
      "height": {
        "type": {
          "name": "union",
          "value": [
            {
              "name": "string"
            },
            {
              "name": "number"
            }
          ]
        },
        "required": false,
        "description": "Sets height of the parent div of the chart",
        "defaultValue": {
          "value": "400",
          "computed": false
        }
      },
      "chartOptions": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "Object containing all chart options\nSee https://tradingview.github.io/lightweight-charts/docs/api/interfaces/ChartOptions for possible options"
      }
    }
  }
}